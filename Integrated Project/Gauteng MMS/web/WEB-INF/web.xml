<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>Handler</servlet-name>
        <servlet-class>servlets.Handler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReferenceListServlet</servlet-name>
        <servlet-class>servlets.ReferenceListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogInServlet</servlet-name>
        <servlet-class>servlets.LogInServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditUser</servlet-name>
        <servlet-class>servlets.EditUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddBodyFile</servlet-name>
        <servlet-class>servlets.AddBodyFile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterForensicSampleServlet</servlet-name>
        <servlet-class>servlets.RegisterForensicSampleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RequestForensicSampleServlet</servlet-name>
        <servlet-class>servlets.RequestForensicSampleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditReferenceListServlet</servlet-name>
        <servlet-class>servlets.EditReferenceListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteReferenceListServlet</servlet-name>
        <servlet-class>servlets.DeleteReferenceListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PropertyServlet</servlet-name>
        <servlet-class>servlets.PropertyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LabRecordServlet</servlet-name>
        <servlet-class>servlets.LabRecordServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostMortemServlet</servlet-name>
        <servlet-class>servlets.PostMortemServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisteredSamples</servlet-name>
        <servlet-class>servlets.RegisteredSamples</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadICD10Table</servlet-name>
        <servlet-class>servlets.LoadICD10Table</servlet-class>
    </servlet>
    <servlet>
 
        <servlet-name>FilterRegionServlet</servlet-name>
        <servlet-class>servlets.FilterRegionServlet</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>LinkBodyContent</servlet-name>
        <servlet-class>servlets.LinkBodyContent</servlet-class>
 
    </servlet>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Handler</servlet-name>
        <url-pattern>/Handler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReferenceListServlet</servlet-name>
        <url-pattern>/ReferenceListServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogInServlet</servlet-name>
        <url-pattern>/LogInServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditUser</servlet-name>
        <url-pattern>/EditUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddBodyFile</servlet-name>
        <url-pattern>/AddBodyFile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterForensicSampleServlet</servlet-name>
        <url-pattern>/RegisterForensicSampleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequestForensicSampleServlet</servlet-name>
        <url-pattern>/RequestForensicSampleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditReferenceListServlet</servlet-name>
        <url-pattern>/EditReferenceListServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteReferenceListServlet</servlet-name>
        <url-pattern>/DeleteReferenceListServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PropertyServlet</servlet-name>
        <url-pattern>/PropertyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LabRecordServlet</servlet-name>
        <url-pattern>/LabRecordServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostMortemServlet</servlet-name>
        <url-pattern>/PostMortemServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisteredSamples</servlet-name>
        <url-pattern>/RegisteredSamples</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadICD10Table</servlet-name>
        <url-pattern>/LoadICD10Table</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
 
        <servlet-name>FilterRegionServlet</servlet-name>
        <url-pattern>/FilterRegionServlet</url-pattern>
   </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LinkBodyContent</servlet-name>
        <url-pattern>/LinkBodyContent</url-pattern>
 
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>faces/Login.jsp</welcome-file>
    </welcome-file-list>
</web-app>
